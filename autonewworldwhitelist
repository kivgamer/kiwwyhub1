_G.x = true
_G.weapon = "Superhuman"


local VirtualUser = game:GetService('VirtualUser')


while _G.x do wait(.1)
MyLevel = game:GetService("Players").LocalPlayer.Data.Level.Value
if MyLevel >= 1500 then
Mon = "rip_indra [Lv. 1500] [Boss]"
LevelQuest = 1
NameQuest = "Area1Quest"
NameMon = "rip_indra"
OldWorld = false
local placeId = game.PlaceId
if placeId == 7449423635 then
           return
           end

          

game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-27024.0546875, 65.898796081543, 337.60375976563)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-27024.0546875, 65.898796081543, 337.60375976563)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-27024.0546875, 65.898796081543, 337.60375976563)

    

    for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
        if v.Name == Mon then
            
            local toolname = _G.weapon -- replace with the name of the tool
    local Plr = game:GetService("Players").LocalPlayer
        if Plr.Backpack:FindFirstChild(toolname) and Plr.Character:FindFirstChild(toolname) == nil then
            local tool = Plr.Backpack:FindFirstChild(toolname)
            Plr.Character.Humanoid:EquipTool(tool)
        end
        end

        
        repeat wait()
            game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)

            VirtualUser:CaptureController()
            VirtualUser:ClickButton1(Vector2.new(851, 158), game:GetService("Workspace").Camera.CFrame)
            v.HumanoidRootPart.Transparency = 1
            v.HumanoidRootPart.Size = Vector3.new(50, 50, 50) -- hitbox
        game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0 , 20, 0) 
        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TravelZou")
    until not v.Parent or v.Humanoid.Health <= 0 or _G.x == false
end
end
end
